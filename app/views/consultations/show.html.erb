<%= link_to "Retour",  patient_consultations_path, class: "btn btn-primary" %>
<%= @patient.nom %>
<%= @patient.prenom %>
<%= @consultation.date %>
<%= @consultation.motif %>
<%= @consultation.examenclinique %>
<%= @consultation.examencomplementaire %>
  <% if @consultation.pictures.attachments.present? %>
    <% @consultation.pictures.each do |consultation| %>
      <%= cl_image_tag consultation.key, height: 600, width: 400, crop: :fill %>
    <%end%>
  <% end %>
